# ─── Wallet ───────────────────────────────────────────────────────────────────
PRIVATE_KEY=your_private_key_here

# ─── RPC Endpoints ────────────────────────────────────────────────────────────
RPC_ETHEREUM=https://mainnet.infura.io/v3/YOUR_INFURA_KEY
RPC_POLYGON=https://polygon-mainnet.infura.io/v3/YOUR_INFURA_KEY
RPC_ARBITRUM=https://arbitrum-mainnet.infura.io/v3/YOUR_INFURA_KEY
RPC_BSC=https://bsc-dataseed1.binance.org/

# Optional WebSocket RPCs for lower latency
WS_ETHEREUM=wss://mainnet.infura.io/ws/v3/YOUR_INFURA_KEY
WS_POLYGON=wss://polygon-mainnet.infura.io/ws/v3/YOUR_INFURA_KEY
WS_ARBITRUM=wss://arbitrum-mainnet.infura.io/ws/v3/YOUR_INFURA_KEY

# ─── Deployed Contract Addresses ─────────────────────────────────────────────
ARB_CONTRACT_ADDRESS_ETHEREUM=0x...
ARB_CONTRACT_ADDRESS_POLYGON=0x...
ARB_CONTRACT_ADDRESS_ARBITRUM=0x...
ARB_CONTRACT_ADDRESS_BSC=0x...

# ─── Bot Settings ─────────────────────────────────────────────────────────────
# Active chain: ethereum | polygon | arbitrum | bsc
CHAIN=ethereum

# Flashloan amount in USD (supports $1 – $1,000,000)
LOAN_AMOUNT_USD=10000

# Minimum arbitrage spread to pursue (in basis points, 1 bps = 0.01%)
MIN_PROFIT_BPS=15

# Scan interval in milliseconds (lower = higher frequency)
SCAN_INTERVAL_MS=2000

# Set to true to execute live trades (false = simulation only)
TRADE_LIVE=false

# ─── Flashbots (Ethereum mainnet only) ───────────────────────────────────────
# Flashbots auth signer key – a SEPARATE random wallet, not your main key
FLASHBOTS_AUTH_KEY=your_flashbots_auth_key_here

# ─── Proxy / Socket Settings (optional) ──────────────────────────────────────
# HTTP_PROXY=http://user:pass@proxy.example.com:8080
# HTTPS_PROXY=https://user:pass@proxy.example.com:8080

# ─── API Keys ─────────────────────────────────────────────────────────────────
INFURA_KEY=your_infura_project_id
ALCHEMY_KEY=your_alchemy_api_key
ETHERSCAN_API_KEY=your_etherscan_api_key
POLYGONSCAN_API_KEY=your_polygonscan_api_key
ARBISCAN_API_KEY=your_arbiscan_api_key
BSCSCAN_API_KEY=your_bscscan_api_key
